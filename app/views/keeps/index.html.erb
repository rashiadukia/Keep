<p id="notice"><%= notice %></p>

<h1>Keeps</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Note</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @keeps.each do |keep| %>
      <tr>
        <td><%= keep.name %></td>
        <td><%= keep.note %></td>
        <td><%= link_to 'Show', keep %></td>
        <td><%= link_to 'Edit', edit_keep_path(keep) %></td>
        <td><%= link_to 'Destroy', keep, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Keep', new_keep_path %>
